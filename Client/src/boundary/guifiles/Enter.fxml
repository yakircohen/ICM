<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="bpEnter" prefHeight="476.0" prefWidth="756.0" style="-fx-opacity: 50;" styleClass="anchor" stylesheets="@img/login.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EnterController">
   <top>
      <MenuBar prefHeight="25.0" prefWidth="194.0" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="ICM">
               <items>
                  <MenuItem fx:id="btnLoginMenu" mnemonicParsing="false" onAction="#loginPaneView" text="Login" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Connection">
               <items>
                  <MenuItem fx:id="btnConfigure" mnemonicParsing="false" onAction="#connectionPaneView" text="Configure Connection" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem mnemonicParsing="false" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <center>
      <AnchorPane fx:id="apInside" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
